#include<stdio.h>
    int main()
{
    int n,i;
    int arr[20];

    printf("Enter the number range:\n");
    scanf("%d",&n);
    arr[0]=-1;
    arr[1]=1;
    printf("\n FIBONACCI SERIES: \n");
    for(i=2;i<=n+1;i++)
    {
       arr[i]=arr[i-1]+arr[i-2];
    printf(" %d",arr[i]);
    }
   return 0;
}
